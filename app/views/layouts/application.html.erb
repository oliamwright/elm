<!DOCTYPE html>
<html>
<head>
  <title><%= @page_title %></title>
  <%= stylesheet_link_tag :all, :recursive => true %>
  <%= javascript_include_tag 'jquery-1.7.1.min.js' %>
	<%= javascript_include_tag 'jquery-ujs' %>
	<%= javascript_include_tag 'jquery-ui-1.8.17.custom.min' %>
	<%= javascript_include_tag 'highcharts' %>
	<%= javascript_include_tag 'jquery.jqote2.js' %>
	<%= javascript_include_tag 'jquery.purr.js' %>
	<%= javascript_include_tag 'jquery.hotkeys.js' %>
	<%= javascript_include_tag 'best_in_place' %>
	<%= javascript_include_tag 'sticky-table' %>
	<%= javascript_include_tag 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>

<%= render :partial => 'shared/page_header' %>

<%= render :partial => 'shared/flash', :object => flash %>

<div id="templates"></div>

<div class="page">
<%= yield %>
</div>

</body>
</html>
